class Solution(object):
    def sortArrayByParity(self, nums):
        odd_nums = []
        even_nums = []
        m_nums = []

        for n in nums:
            if n % 2 == 0:
                even_nums.append(n)
            else:
                odd_nums.append(n)
        m_nums.extend(even_nums)
        m_nums.extend(odd_nums)
        return m_nums
